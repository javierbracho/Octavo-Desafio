            <div class="card mb-4">
                <img src="{{this.image}}" class="card-img-top" alt="Product Image"> <!-- Agrega la imagen aquÃ­ -->
                <div class="card-body">
                    <h5 class="card-title">{{this.title}}</h5>
                    <p class="card-text">{{this.description}}</p>
                    <p class="card-price">Price: {{this.price}}</p>
                    <p class="card-stock">Stock: {{this.stock}}</p>
